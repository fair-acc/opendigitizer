blocks:
  - name: sum sigs
    id: opendigitizer::Arithmetic
  - name: FFT
    id: gr::blocks::fft::FFT
  - name: sine source 1
    id: opendigitizer::SineSource
    parameters:
      frequency: 1.000000
  - name: sine source 3
    id: opendigitizer::SineSource
    parameters:
      frequency: 1.300000
  - name: sink 1
    id: opendigitizer::ImPlotSink
    parameters:
      color: 0xff0000
  - name: sink 2
    id: opendigitizer::ImPlotSink
    parameters:
      color: 0x00ff00
  - name: sink 3
    id: opendigitizer::ImPlotSink
    parameters:
      color: 0x0000ff
  - name: sink 4
    id: opendigitizer::ImPlotSink
    parameters:
      color: 0xffff00
connections:
  - [sine source 1, 0, sum sigs, 0]
  - [sine source 3, 0, sum sigs, 1]
  - [sum sigs, 0, FFT, 0]
  - [sum sigs, 0, sink 1, 0]
  - [FFT, 0, sink 2, 0]
  - [sine source 1, 0, sink 3, 0]
  - [sine source 3, 0, sink 4, 0]
